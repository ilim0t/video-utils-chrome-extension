{
  "name": "Video Utils",
  "version": "0.1",
  "description": "It provides a useful feature for watching videos.",
  "manifest_version": 2,
  "permissions": ["tabs"],
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "run_at": "document_end",
      "js": ["contentScript.js"]
    }
  ],
  "web_accessible_resources": ["default_sites.json"]
}
