{
  "name": "Video Utils",
  "version": "0.1",
  "description": "It provides a useful feature for watching videos.",
  "manifest_version": 3,
  "permissions": ["tabs", "storage"],
  "action": {
    "default_popup": "popup.html"
  },
  "background": {
    "service_worker": "background.js"
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "run_at": "document_end",
      "js": ["contentScript.js"]
    }
  ],
  "web_accessible_resources": [{
    "resources": ["default_sites.json"],
    "matches": ["<all_urls>"]
  }]
}
